<!-- Main Content -->
<div>
  <h1>Tweet Management</h1>
</div>
<div>
  <table class="table">
    <thead>
      <tr>
        <td>User</td>
        <td>Description</td>
        <td>Reply</td>
        <td colspan="1"></td>
      </tr>
    </thead>
    <tbody>
      {{#each tweets }}
        <tr>
          <td>{{this.username}}</td>
          <td>{{this.description}}</td>
          <td>{{this.comment}}</td>
          <td><a href="#">Delete</a></td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>